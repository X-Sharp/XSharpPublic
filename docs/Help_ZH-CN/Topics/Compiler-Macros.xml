    <keyword>__VO5__</keyword>
    <keyword>__VO6__</keyword>
    <keyword>__VO7__</keyword>
    <keyword>__VO8__</keyword>
    <keyword>__VO9__</keyword>
    <keyword>__WINDIR__</keyword>
