{
  "$schema": "http://json.schemastore.org/template",
  "Author": "XSharp",
  "Classifications": [
    "Windows",
    "Console",
    "Vulcan"
  ],
  "Identity": "XSharp.VulcanConsole",
  "Name": "Vulcan Application",
  "Description": "A project for creating a X# command-line application in the Vulcan dialect.",
  "SourceName": "XSharp.VulcanConsole",
  "ShortName": "xsvulcanconsole",
  "Tags": {
    "language": "X#",
    "type": "project",
    "icon": ".\VulcanConsole.png",
    "is-icon-path": "true"
  },
  "PreferNameDirectory": true,
  "DefaultName": "VulcanApplication",
  "Symbols": {
    "TargetNetFramework": {
      "Type": "parameter",
      "Description": "Target .NET version",
      "Datatype": "choice",
      "Choices": [
        {
          "Choice": "net8.0",
          "Description": "Target .NET 8"
        },
        {
          "Choice": "net9.0",
          "Description": "Target .NET 9"
        },
        {
          "Choice": "net10.0",
          "Description": "Target .NET 10"
        }
      ],
      "Replaces": "net8.0",
      "DefaultValue": "net8.0"
    },
    "DefaultNamespace": {
      "Type": "parameter",
      "Datatype": "string",
      "Replaces": "Company.Namespace1",
      "DefaultValue": "Company.Namespace1"
    }
  }
}