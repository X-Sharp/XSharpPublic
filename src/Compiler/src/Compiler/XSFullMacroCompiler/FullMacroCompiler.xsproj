<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <UseXSharpKey>true</UseXSharpKey>
    <SignAssembly>false</SignAssembly>
    <StrongNameKeyId />
    <CommonDir>$(Solutiondir)Common\</CommonDir>
    <ArtifactsDir>$(SolutionDir)..\Artifacts\</ArtifactsDir>
    <RtOutPutDir>$(ArtifactsDir)$(Config)\net46\</RtOutPutDir>
    <XsiDir>$(ArtifactsDir)bin\xsi\$(Config)\net472\</XsiDir>
    <AssemblyOriginatorKeyFile>$(Solutiondir)common\XSharp.snk</AssemblyOriginatorKeyFile>
    <PublicKeyToken>ed555a0467764586</PublicKeyToken>
    <PublicKey>
      
    </PublicKey>
    <NoWin32Manifest>False</NoWin32Manifest>
    <RuntimeFilesDir Condition="'$(RuntimeFilesDir)' == '' ">$(DevRtDir)Binaries\Debug\</RuntimeFilesDir>
    <EnableBaseIntermediateOutputPathMismatchWarning>false</EnableBaseIntermediateOutputPathMismatchWarning>
    <BinariesDir>$(ArtifactsDir)bin\</BinariesDir>
    <RuntimeIdentifiers>win</RuntimeIdentifiers>
    <Vo15>true</Vo15>
    <Allowdot>false</Allowdot>
	<ResolveNuGetPackages>false</ResolveNuGetPackages>
  </PropertyGroup>
  <Import Project="$(XSharpMsBuildDir)\XSharp.Default.props" />
  <PropertyGroup>
    <ProjectGuid>5dc40adc-f250-4611-ad89-b67b150bbc8e</ProjectGuid>
    <OutputType>Library</OutputType>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>XSharp.MacroCompiler</RootNamespace>
    <AssemblyName>XSharp.MacroCompiler.Full</AssemblyName>
    <TargetFrameworkVersion>v4.8</TargetFrameworkVersion>
    <Name>FullMacroCompiler</Name>
    <Nologo>true</Nologo>
    <GenerateFullPaths>true</GenerateFullPaths>
    <Dialect>VO</Dialect>
    <VulcanCompatibleResources>False</VulcanCompatibleResources>
    <TargetFrameworkProfile />
    <SignAssembly>true</SignAssembly>
    <IncludePaths>$(CommonDir)</IncludePaths>
    <NoStandardDefs>true</NoStandardDefs>
    <StartupObject />
    <ApplicationIcon />
    <AZ>false</AZ>
    <CS>true</CS>
    <LB>false</LB>
    <NamedArgs>false</NamedArgs>
    <OVF>false</OVF>
    <Unsafe>false</Unsafe>
    <INS>false</INS>
    <NS>false</NS>
    <StandardDefs />
    <MemVar>false</MemVar>
    <Undeclared>false</Undeclared>
    <InitLocals>false</InitLocals>
    <Prefer32Bit>False</Prefer32Bit>
    <CommandLineOption />
    <PPO>false</PPO>
    <OutputPath>$(BinariesDir)$(ProjectName)\$(Configuration)</OutputPath>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <UseSharedCompilation>false</UseSharedCompilation>
    <NoWarn>MSB3277</NoWarn>
    <SignAssembly>true</SignAssembly>
    <DelaySign>false</DelaySign>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>False</TreatWarningsAsErrors>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <EmitDebugInformation>true</EmitDebugInformation>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <DefineConstants>DEBUG;TRACE;RUNTIME</DefineConstants>
    <WarningsAsErrors />
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|AnyCPU'">
    <EmitDebugInformation>false</EmitDebugInformation>
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <DefineConstants>TRACE;RUNTIME</DefineConstants>
    <WarningsAsErrors />
  </PropertyGroup>
  <Import Project="$(XSharpMsBuildDir)\XSharp.targets" />
  <ItemGroup>
    <Compile Include="ExecScript.prg" />
    <Compile Include="Properties\AssemblyInfo.prg">
      <SubType>Code</SubType>
    </Compile>
    <Compile Include="MacroCompiler.prg">
      <SubType>Code</SubType>
    </Compile>
    <Compile Include="ScriptCache.prg">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="e:\XSharp\Dev\Artifacts\Debug\net46\XSharp.Core.dll" />
    <Reference Include="e:\XSharp\Dev\Artifacts\Debug\net46\XSharp.RT.dll" />
    <Reference Include="System.Collections.Immutable, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
      <Name>System.Collections.Immutable</Name>
      <AssemblyName>System.Collections.Immutable.dll</AssemblyName>
      <SpecificVersion>False</SpecificVersion>
      <Private>True</Private>
      <HintPath>..\..\..\..\..\Artifacts\bin\xsi\Debug\net472\System.Collections.Immutable.dll</HintPath>
    </Reference>
    <Reference Include="XSharp.CodeAnalysis, Version=3.0.0.0, Culture=neutral, PublicKeyToken=ed555a0467764586">
      <Name>XSharp.CodeAnalysis</Name>
      <AssemblyName>XSharp.CodeAnalysis.dll</AssemblyName>
      <SpecificVersion>False</SpecificVersion>
      <Private>True</Private>
      <HintPath>..\..\..\..\..\Artifacts\bin\xsi\Debug\net472\XSharp.CodeAnalysis.dll</HintPath>
    </Reference>
    <Reference Include="XSharp.Scripting, Version=3.0.0.0, Culture=neutral, PublicKeyToken=ed555a0467764586">
      <Name>XSharp.Scripting</Name>
      <AssemblyName>XSharp.Scripting.dll</AssemblyName>
      <SpecificVersion>False</SpecificVersion>
      <Private>True</Private>
      <HintPath>..\..\..\..\..\Artifacts\bin\xsi\Debug\net472\XSharp.Scripting.dll</HintPath>
    </Reference>
  </ItemGroup>
  <Target Name="SignWithXSharpKey" AfterTargets="PostBuildEvent" Condition="'$(UseXSharpKey)' == 'true'">
    <Exec Command="&quot;$(SDK40ToolsPath)sn.exe&quot;  -q -R $(TargetPath) $(AssemblyOriginatorKeyFile)" />
  </Target>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <PreBuildEvent />
    <PostBuildEvent />
    <RunPostBuildEvent />
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|AnyCPU'">
    <PreBuildEvent />
    <PostBuildEvent />
    <RunPostBuildEvent />
  </PropertyGroup>
</Project>