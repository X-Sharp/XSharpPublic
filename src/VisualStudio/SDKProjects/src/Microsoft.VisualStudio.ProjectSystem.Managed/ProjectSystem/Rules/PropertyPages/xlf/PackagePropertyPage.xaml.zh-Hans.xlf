<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="zh-Hans" original="../PackagePropertyPage.xaml">
    <body>
      <trans-unit id="BoolProperty|GeneratePackageOnBuild|Description">
        <source>Produce a package file during build operations.</source>
        <target state="translated">在生成操作期间创建包文件。</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|GeneratePackageOnBuild|DisplayName">
        <source>Generate NuGet package on build</source>
        <target state="translated">在构建时生成 NuGet 包</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|IncludeSymbols|Description">
        <source>Create an additional symbol package when the project is packed.</source>
        <target state="translated">打包项目时创建其他符号包。</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|IncludeSymbols|DisplayName">
        <source>Produce a symbol package</source>
        <target state="translated">生成符号包</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|PackAsTool|Description">
        <source>Packs this project as a special package that contains a console application that may be installed via the "dotnet tool" command.</source>
        <target state="translated">将此项目打包为专用包，它包含可通过 “dotnet tool”命令安装的控制台应用程序。</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|PackAsTool|DisplayName">
        <source>Pack as a .NET tool</source>
        <target state="translated">打包为 .NET 工具</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|PackageRequireLicenseAcceptance|Description">
        <source>Prompt the user to accept the license when installing this package.</source>
        <target state="translated">安装此包时，提示用户接受许可证。</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|PackageRequireLicenseAcceptance|DisplayName">
        <source>Require license acceptance</source>
        <target state="translated">要求接受许可证</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|General|Description">
        <source>General settings for the package.</source>
        <target state="translated">包的常规设置。</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|General|DisplayName">
        <source>General</source>
        <target state="translated">常规</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|License|Description">
        <source>Specifies the license for the package.</source>
        <target state="translated">指定包的许可证。</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|License|DisplayName">
        <source>License</source>
        <target state="translated">许可证</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|Symbols|Description">
        <source>Specifies how symbols are added to the package.</source>
        <target state="translated">指定如何将符号添加到包中。</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|Symbols|DisplayName">
        <source>Symbols</source>
        <target state="translated">符号</target>
        <note />
      </trans-unit>
      <trans-unit id="DynamicEnumProperty|NeutralLanguage|DisplayName">
        <source>Assembly neutral language</source>
        <target state="translated">程序集非特定语言</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|PackageLicenseKind|Description">
        <source>Specify a license for the project's package.</source>
        <target state="translated">指定项目包的许可证。</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|PackageLicenseKind|DisplayName">
        <source>Package License</source>
        <target state="translated">包许可证</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|SymbolPackageFormat|Description">
        <source>Specifies the format of the symbols package.</source>
        <target state="translated">指定符号包的格式。</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|SymbolPackageFormat|DisplayName">
        <source>Symbol package format</source>
        <target state="translated">符号包格式</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|PackageLicenseKind.Expression|DisplayName">
        <source>SPDX License Expression</source>
        <target state="translated">SPDX 许可证表达式</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|PackageLicenseKind.File|DisplayName">
        <source>Embedded File</source>
        <target state="translated">嵌入的文件</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|PackageLicenseKind.None|DisplayName">
        <source>None</source>
        <target state="translated">无</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|SymbolPackageFormat.snupkg|DisplayName">
        <source>snupkg</source>
        <target state="translated">snupkg</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|SymbolPackageFormat.symbols.nupkg|DisplayName">
        <source>symbols.nupkg (legacy)</source>
        <target state="translated">symbols.nupkg (旧版)</target>
        <note />
      </trans-unit>
      <trans-unit id="Rule|Package|Description">
        <source>Properties related to producing NuGet packages.</source>
        <target state="translated">与生成 NuGet 包相关的属性。</target>
        <note />
      </trans-unit>
      <trans-unit id="Rule|Package|DisplayName">
        <source>Package</source>
        <target state="translated">包</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|AssemblyVersion|DisplayName">
        <source>Assembly version</source>
        <target state="translated">程序集版本</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Authors|Description">
        <source>A comma-separated list of packages authors, matching the profile names on nuget.org. These are displayed in the NuGet Gallery on nuget.org and are used to cross-reference packages by the same authors.</source>
        <target state="translated">以逗号分隔的包作者列表，与 nuget.org 上的配置文件名称匹配。这些项在 nuget.org 上的 NuGet 库中显示，用于按相同的作者交叉引用包。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Authors|DisplayName">
        <source>Authors</source>
        <target state="translated">作者</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Company|DisplayName">
        <source>Company</source>
        <target state="translated">公司</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Copyright|Description">
        <source>Copyright details for the package.</source>
        <target state="translated">包的版权详细信息</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Copyright|DisplayName">
        <source>Copyright</source>
        <target state="translated">版权</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Description|Description">
        <source>A description of the package for UI display.</source>
        <target state="translated">用于 UI 显示的包的说明。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Description|DisplayName">
        <source>Description</source>
        <target state="translated">说明</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|FileVersion|DisplayName">
        <source>File version</source>
        <target state="translated">文件版本</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageIcon|Description">
        <source>The icon image for the package. Image file size is limited to 1 MB. Supported file formats include JPEG and PNG. An image resolution of 128x128 is recommended.</source>
        <target state="translated">包的图标图像。图像文件大小限制为 1 MB。受支持的文件格式包含 JPEG 和 PNG。推荐使用 128x128 的图像分辨率。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageIcon|DisplayName">
        <source>Icon</source>
        <target state="translated">图标</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageId|Description">
        <source>The case-insensitive package identifier, which must be unique across nuget.org or whatever gallery the package resides in. IDs may not contain spaces or characters that are not valid for a URL, and generally follow .NET namespace rules.</source>
        <target state="translated">不区分大小写的包标识符，该标识符在 nuget.org 或包所在的任何库中必须是唯一的。ID 不能包含对于 URL 无效的空格或字符，且通常遵循 .NET 命名空间规则。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageId|DisplayName">
        <source>Package ID</source>
        <target state="translated">包 ID</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageLicenseExpression|Description">
        <source>An SPDX license expression, often shown in UIs like nuget.org.</source>
        <target state="translated">SPDX 许可证表达式，通常在 nuget.org 等 UI 中显示。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageLicenseExpression|DisplayName">
        <source>License expression</source>
        <target state="translated">许可证表达式</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageLicenseFile|DisplayName">
        <source>License file</source>
        <target state="translated">许可证文件</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageOutputPath|Description">
        <source>Determines the output path in which the package will be dropped.</source>
        <target state="translated">确定将在其中删除包的输出路径。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageOutputPath|DisplayName">
        <source>Package Output Path</source>
        <target state="translated">包输出路径</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageProjectUrl|Description">
        <source>A URL for the package's home page, often shown in UI displays as well as nuget.org.</source>
        <target state="translated">包主页的 URL，通常显示在 UI 和 nuget.org 中。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageProjectUrl|DisplayName">
        <source>Project URL</source>
        <target state="translated">项目 URL</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageReadmeFile|Description">
        <source>The README document for the package. Must be a Markdown (.md) file.</source>
        <target state="translated">包的自述文件。必须为 Markdown (.md)文件。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageReadmeFile|DisplayName">
        <source>README</source>
        <target state="translated">自述文件</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageReleaseNotes|Description">
        <source>A description of the changes made in this release of the package, often used in UI like the Updates tab of the Visual Studio Package Manager in place of the package description.</source>
        <target state="translated">此版本包中所作更改的说明，通常代替包说明用在 UI 中，如 Visual Studio 包管理器的“更新”选项卡。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageReleaseNotes|DisplayName">
        <source>Release notes</source>
        <target state="translated">发行说明</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageTags|Description">
        <source>A semicolon-delimited list of tags and keywords that describe the package and aid discoverability of packages through search and filtering.</source>
        <target state="translated">以分号分隔的标记和关键字列表，描述包并通过搜索和筛选辅助包的可发现性。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|PackageTags|DisplayName">
        <source>Tags</source>
        <target state="translated">标记</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Product|DisplayName">
        <source>Product</source>
        <target state="translated">产品</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|ReadAboutSpdxExpressions|DisplayName">
        <source>Read about SPDX license expressions</source>
        <target state="translated">阅读 SPDX 许可证表达式</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|RepositoryType|Description">
        <source>Specifies the type of the repository. Default is 'git'.</source>
        <target state="translated">指定仓库的类型。默认值为 'git'。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|RepositoryType|DisplayName">
        <source>Repository type</source>
        <target state="translated">仓库类型</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|RepositoryUrl|Description">
        <source>Specifies the URL for the repository where the source code for the package resides and/or from which it's being built. For linking to the project page, use the 'Project URL' field, instead.</source>
        <target state="translated">指定包的源代码所在的仓库和/或从其中构建源代码的仓库的 URL。如需链接到项目页面，请改用“项目 URL”字段。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|RepositoryUrl|DisplayName">
        <source>Repository URL</source>
        <target state="translated">仓库 URL</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Title|Description">
        <source>A human-friendly title of the package, typically used in UI displays as on nuget.org and the Package Manager in Visual Studio.</source>
        <target state="translated">包的易记标题，通常在 UI 显示中使用，如 nuget.org 和 Visual Studio 中的包管理器上。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Title|DisplayName">
        <source>Title</source>
        <target state="translated">标题</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|ToolCommandName|Description">
        <source>The command name via which the .NET tool will be invoked on the command line.</source>
        <target state="translated">将在命令行上调用 .NET 工具的命令名称。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|ToolCommandName|DisplayName">
        <source>.NET tool command name</source>
        <target state="translated">.NET 工具命令名称</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Version|Description">
        <source>The version of the package, following the major.minor.patch pattern. Version numbers may include a pre-release suffix.</source>
        <target state="translated">包的版本，遵循 major.minor.patch 模式。版本号可能包括预发布后缀。</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Version|DisplayName">
        <source>Package Version</source>
        <target state="translated">包版本</target>
        <note />
      </trans-unit>
    </body>
